<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="suite">
	<!-- 填写启动浏览器名字：chrome、firefox、ie、ghost，推荐chrome -->
	<parameter name="browserName" value="chrome" />
	<!-- 填写浏览器驱动存放路径的属性配置文件位置 -->
	<parameter name="driverConfigFilePath" value="../src/test/resources/config/driver.properties" />
	<!-- 填写浏览器驱动存放路径的属性配置文件位置 -->
	<parameter name="cookiesConfigFilePath" value="../src/test/resources/config/cookies.txt" />
	<!-- 填写AutoIT的exe执行文件位置 -->
	<parameter name="autoitFolderPath" value="../src/test/resources/autoit/" />
	<!-- 填写测试的网站url -->
	<parameter name="testurl" value="https://mail.126.com" />
	<!-- 填写页面加载等待超时时间 -->
	<parameter name="pageLoadTimeout" value="20" />
	<!-- 填写保留测试日志天数 -->
	<parameter name="keepLogDay" value="10" />
	<test name="test" preserve-order="true">
		<classes>
			<class name="com.demo.cases.login.LoginPage_001_LoginSuccess" />
			<class name="com.demo.cases.home.HomePage_001_Overview_Tab" />
		</classes>
	</test>
</suite>